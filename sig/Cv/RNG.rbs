module Cv
  class RNG
    attr_accessor state: Integer

    def initialize: () -> NilClass
               | (state: Integer) -> NilClass
    def ==: (other: Cv::RNG) -> TrueClass
    def call: () -> Integer
         | (n: Integer) -> Integer
    def fill: (mat: Cv::InputOutputArray, dist_type: Integer, a: Cv::InputArray, b: Cv::InputArray, saturate_range: TrueClass) -> NilClass
    def gaussian: (sigma: Float) -> Float
    def next: () -> Integer
    def uniform: (a: Float, b: Float) -> Float
            | (a: Float, b: Float) -> Float
            | (a: Integer, b: Integer) -> Integer
  end
end
